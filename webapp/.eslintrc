// .eslintrc
{
  parser: "babel-eslint",
  "extends": [ "eslint: recommended", "plugin: react/recommended" ], 
  "globals": { 
    "window": true, 
    "document": true, 
    "module": true, 
    "require": true,
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
     },
  "plugins": [
  "react",
  "jsx-ally",
  "import"
  ],
  "rules": {
  "max-len": [1, 120, 2, {ignoreComments: true}],
  "prop-types": [0],
  "no-console": 1,
  "react/jsx-filename-extension": [1,{"extensions":["js","jsx"]}]
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "commonjs": true
  }
}